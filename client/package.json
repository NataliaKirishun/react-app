    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga|less)$": "<rootDir>/assetsTransformer.js"
    },
    "coverageReporters": [
      "text",
      "cobertura"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/cypress"
    ]
  },
